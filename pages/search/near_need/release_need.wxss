@import "/common/weui.wxss";

.weui-cells {
  font-size: 28rpx;
  font-weight: normal;
  letter-spacing: 0rpx;
  color: #333;
}

.phcolor {
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 91rpx;
  letter-spacing: 0rpx;
  color: #ccc;
}

.btn {
  width: 92%;
  margin: 20rpx auto 30rpx;
}

.new_btn_css {
  font-size: 36rpx;
  color: #fff;
  background: -webkit-gradient(linear, 0% 0%, 100% 0%, from(rgba(249, 101, 90, 1)), to(rgba(237, 63, 49, 1))) !important;
}

.cell_img {
  padding: 40rpx 30rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.textarea_view {
  padding-top: 20rpx;
  padding-left: 30rpx;
}

.css_bd {
  width: 2px;
  text-align: center;
}

.time_class {
  padding: 4rpx 30rpx;
}

.money_class {
  padding: 4rpx 30rpx;
}

.weui-uploader__input-box {
  width: 220rpx;
  height: 220rpx;
  border: 1px dashed #d9d9d9;
}

.weui-cells:after {
  border-bottom: 1rpx solid #f2f2f2;
  margin-left: 30rpx;
}

.weui-cells:before {
  border-top: 1rpx solid #f2f2f2;
  margin-left: 30rpx;
}

.weui-cell:after {
  border-bottom: 1rpx solid #f2f2f2;
}

.weui-cell:before {
  border-top: 1rpx solid #f2f2f2;
}

