@import "/style/weui.wxss";
Page{
  background-color: #fff;
  padding-bottom: 50px;
}

.weui-label{
  font-size: 28rpx;
	color: #333333;
}

.phcolor{
  font-size: 28rpx;
	color: #cccccc;
}

.idcard{
  text-align: center;
  margin: 30px;
}


.idcard image{
  width: 494rpx;
  height: 276rpx;
}

.id_text{
  font-size: 30rpx;
	color: #333333;
  margin-bottom: 20px;
}

.status{
  text-align: center;
  color: red;
  font-size: 30rpx;
  margin: 10px;
}

.btn_class{
  margin-top: 30px;
  text-align: center;
}

.btn_class button{
  margin-left: 15px;
  margin-right: 15px;
  
}

.weui-cell:after {
  border-bottom: 1rpx solid #f0f0f0;
}

.weui-cell:before {
  border-top: 1rpx solid #f0f0f0;
}