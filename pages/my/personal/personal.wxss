@import "/style/weui.wxss";

Page {
  background-color: #f6f7f8;
}

.photo {
  width: 100%;
  height: 150px;
  text-align: center;
  display: table;
  background-color: #fff;
}

.photo_class {
  vertical-align: middle;
  display: table-cell;
}

.userinfo-avatar {
  width: 188rpx;
  height: 188rpx;
  border-radius: 50%;
}

.title_class{
   background-color: #fff;
}

.lable_name{
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #333333;
}

.value_class{
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #333;
}

.weui-cell:after {
  border-bottom: 1rpx solid #f0f0f0;
}

.weui-cell:before {
  border-top: 1rpx solid #f0f0f0;
}

.btn_class{
  text-align: center;
}

.btn{
  margin-left: 15px;
  margin-right: 15px;
  font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #ffffff;
}

